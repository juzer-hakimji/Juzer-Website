@model ViewModel.MainUserVM

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title></title>
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=PT+Sans:400,700' type='text/css">
    @*<link rel="stylesheet" href="~/Content/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/HomePage/animate.css">
    <link rel="stylesheet" href="~/Content/HomePage/magnific-popup.css">
    <link rel="stylesheet" href="~/Content/HomePage/font-awesome.min.css">
    <link href="~/Content/css/select2.min.css" rel="stylesheet" />
    <link href="~/Content/HomePage/reset.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/HomePage/style.css">*@
    @Styles.Render("~/StyleBundle/HomePage")
</head>
<body data-spy="scroll" data-target=".navbar-collapse" data-offset="50">
    <!-- PRE LOADER -->
    <div class="preloader">
        <div class="spinner">
            <span class="spinner-rotate"></span>
        </div>
    </div>
    <!-- NAVIGATION SECTION -->
    <div class="navbar custom-navbar navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <button class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon icon-bar"></span>
                    <span class="icon icon-bar"></span>
                    <span class="icon icon-bar"></span>
                </button>
                <!-- lOGO TEXT HERE -->
                <a href="index.html" class="navbar-brand">Juzer Hakimji</a>
            </div>
            <div class="collapse navbar-collapse">
                <ul class="nav navbar-nav navbar-right">
                    <li><a class="smoothScroll" href="#home">Home</a></li>
                    <li><a class="smoothScroll" href="#about">About Juzer Hakimji</a></li>
                    <li><a class="smoothScroll" href="#work">My Works</a></li>
                    <li><a class="smoothScroll" href="#contact">Contact</a></li>
                    <li><a class="UserPopUp" href="#">Sign in/Sign up</a></li>
                </ul>
            </div>
        </div>
    </div>
    <div class="cd-user-modal">
        <!-- this is the entire modal form, including the background -->
        <div class="cd-user-modal-container">
            <!-- this is the container wrapper -->
            <ul class="cd-switcher">
                <li><a href="#0">Sign in</a></li>
                <li><a href="#0">New account</a></li>
            </ul>
            <!-- log in form -->
            @Html.Partial("_Login", Model.UserLoginVM)
            <!-- cd-login -->
            <!-- sign up form -->
            @Html.Partial("_SignUp", Model.UserDetailsVM)
            <!-- cd-signup -->
            <!-- reset password form -->
            @Html.Partial("_ResetPassword", Model.ResetPasswordVM)
            <!-- cd-reset-password -->
            <a href="#0" class="cd-close-form">Close</a>
        </div>
        <!-- cd-user-modal-container -->
    </div>
    <!-- cd-user-modal -->
    <!-- HOME SECTION -->
    <section id="home">
        <div class="container">
            <div class="row">
                <div class="col-md-offset-1 col-md-2 col-sm-3">
                    <img src="~/Content/Images/profile-image.jpg" class="wow fadeInUp img-responsive img-circle" data-wow-delay="0.2s" alt="about image">
                </div>
                <div class="col-md-8 col-sm-8">
                    <h1 class="wow fadeInUp" data-wow-delay="0.6s">Hey there, My name is Juzer Hakimji and I'm a Web Developer.</h1>
                    <a href="/Home/DownloadResume" target="_blank" class="wow fadeInUp btn btn-default section-btn" data-wow-delay="1s">Download Resume</a>
                </div>
            </div>
        </div>
    </section>
    <!-- ABOUT SECTION -->
    <section id="about">
        <div class="container">
            <div class="row">
                <div class="col-md-8 col-sm-12">
                    <div class="about-thumb">
                        <div class="wow fadeInUp section-title" data-wow-delay="0.6s">
                            <h2>A Little Bit Of My Story</h2>
                            <p><strong>Senior Developer at <a href="http://www.xanthus.in" target="_blank">xanthus software solutions</a></strong></p>
                        </div>
                        <div class="wow fadeInUp" data-wow-delay="0.8s">
                            <p>I am also a Freelance web developer with extensive experience in designing and developing enterprise-scale applications.</p>
                        </div>
                        <ul class="social-icon">
                            <li class="wow fadeInLeft" data-wow-delay="1s"><a href="https://twitter.com/juzer_hakimji" class="fa fa-twitter" target="_blank"></a></li>
                            <li class="wow fadeInUp" data-wow-delay="1.2s"><a href="https://www.linkedin.com/in/juzer-hakimji-685293143" class="fa fa-linkedin" target="_blank"></a></li>
                            @*<li class="wow fadeInRight" data-wow-delay="1.4s"><a href="#" class="fa fa-dribbble"></a></li>*@
                        </ul>
                    </div>
                </div>
                <div class="col-md-4 col-sm-12">
                    <div class="wow fadeInUp" data-wow-delay="0.4s">
                        <h2>Hobbies</h2>
                    </div>
                    <div class="wow fadeInUp" data-wow-delay="0.6s">
                        <a href="https://www.instagram.com/p/cqED9yjN0j/">
                            <img src="~/Content/Images/instagram-image1.jpg" class="img-responsive" alt="instagram image">
                        </a>
                        <a href="https://www.instagram.com/p/enL0SsDN51/">
                            <img src="~/Content/Images/instagram-image2.jpg" class="img-responsive" alt="instagram image">
                        </a>
                        <a href="https://www.instagram.com/p/apOuRTDNwJ/">
                            <img src="~/Content/Images/instagram-image3.jpg" class="img-responsive" alt="instagram image">
                        </a>
                        <a href="https://www.instagram.com/p/ZXIj33jN2v/">
                            <img src="~/Content/Images/instagram-image4.jpg" class="img-responsive" alt="instagram image">
                        </a>
                        <a href="https://www.instagram.com/p/Xwev1FjN9-/">
                            <img src="~/Content/Images/instagram-image5.jpg" class="img-responsive" alt="instagram image">
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- SKILL SECTION -->
    <section id="skill">
        <div class="container">
            <div class="row">
                <div class="col-md-4 col-sm-6">
                    <div class="wow fadeInUp section-title" data-wow-delay="0.2s">
                        <h2>Expertise</h2>
                        <p>I am a full stack developer with expertise in microsoft technologies.</p>
                    </div>
                </div>
                <div class="col-md-offset-1 col-md-6 col-sm-6">
                    <div class="skill-thumb">
                        <div class="wow fadeInUp" data-wow-delay="1s">
                            <strong>Asp.net MVC</strong>
                            <span class="pull-right">95%</span>
                            <div class="progress">
                                <div class="progress-bar progress-bar-primary" role="progressbar" aria-valuenow="95" aria-valuemin="0" aria-valuemax="100" style="width: 90%;"></div>
                            </div>
                        </div>
                        <div class="wow fadeInUp" data-wow-delay="1.2s">
                            <strong>SQL Server</strong>
                            <span class="pull-right">100%</span>
                            <div class="progress">
                                <div class="progress-bar progress-bar-primary" role="progressbar" aria-valuenow="80" aria-valuemin="0" aria-valuemax="100" style="width: 100%;"></div>
                            </div>
                        </div>
                        <div class="wow fadeInUp" data-wow-delay="1.4s">
                            <strong>Front End - Javascript,Html,Css</strong>
                            <span class="pull-right">90%</span>
                            <div class="progress">
                                <div class="progress-bar progress-bar-primary" role="progressbar" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100" style="width: 85%;"></div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </section>
    <!-- WORK SECTION -->
    <section id="work">
        <div class="container">
            <div class="row">
                <div class="col-md-12 col-sm-12">
                    <div class="wow fadeInUp section-title" data-wow-delay="0.2s">
                        <h2>About this website</h2>
                        <p>After signing up you can create notes, list your incomes and expenses and manage them by comparing them using graphs.</p>
                    </div>
                </div>
                <div class="wow fadeInUp col-md-3 col-sm-6" data-wow-delay="0.4s">
                    <!-- WORK THUMB -->
                    <div class="work-thumb">
                        <a href="~/Content/Images/Notes.png" class="image-popup">
                            <img src="~/Content/Images/Notes.png" class="img-responsive" alt="Work">
                        </a>
                    </div>
                    <h4>Notes</h4>
                </div>
                <div class="wow fadeInUp col-md-3 col-sm-6" data-wow-delay="0.6s">
                    <!-- WORK THUMB -->
                    <div class="work-thumb">
                        <a href="~/Content/Images/IncomesAndExpenses.jpg" class="image-popup">
                            <img src="~/Content/Images/IncomesAndExpenses.jpg" class="img-responsive" alt="Work">
                        </a>
                    </div>
                    <h4>Incomes and Expenses</h4>
                </div>
                <div class="wow fadeInUp col-md-3 col-sm-6" data-wow-delay="0.8s">
                    <!-- WORK THUMB -->
                    <div class="work-thumb">
                        <a href="~/Content/Images/Graphs.jpg" class="image-popup">
                            <img src="~/Content/Images/Graphs.jpg" class="img-responsive" alt="Work">
                        </a>
                    </div>
                    <h4>Graphs</h4>
                </div>
                <div class="wow fadeInUp col-md-3 col-sm-6" data-wow-delay="0.8s">
                    <!-- WORK THUMB -->
                    <div class="work-thumb">
                        <a href="~/Content/Images/ImportantNote.png" class="image-popup">
                            <img src="~/Content/Images/ImportantNote.png" class="img-responsive" alt="Work">
                        </a>
                    </div>
                    <h4>Important Notes and I&E comparison at one place</h4>
                </div>
            </div>
        </div>
    </section>
    <!-- CONTACT SECTION -->
    <section id="contact">
        <div class="container">
            <div class="row">
                <div class="col-md-12 col-sm-12">
                    <div class="wow fadeInUp section-title" data-wow-delay="0.2s">
                        <h2>You got projects?</h2>
                        <p>Let me help you solve your problems.</p>
                    </div>
                </div>
                <div class="col-md-12 col-sm-12">
                    <!-- CONTACT FORM HERE -->
                    @Html.Partial("_ContactInfo", Model.ContactVM)
                </div>
            </div>
        </div>
    </section>
    <!-- SCRIPTS -->
    @*@Scripts.Render("~/bundles/jquery")
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/HomePage/smoothscroll.js"></script>
    <script src="~/Scripts/HomePage/jquery.magnific-popup.min.js"></script>
    <script src="~/Scripts/HomePage/magnific-popup-options.js"></script>
    <script src="~/Scripts/HomePage/wow.min.js"></script>
    <script src="~/Scripts/select2.min.js"></script>
    <script src="~/Scripts/bootstrap-notify.min.js"></script>
    <script src="~/Scripts/CustomJSScripts/Common.js"></script>
    <script src="~/Scripts/HomePage/script.js"></script>
    <script src="~/Scripts/modernizr-2.8.3.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>*@
    @Scripts.Render("~/ScriptBundle/HomePage")
</body>
</html>

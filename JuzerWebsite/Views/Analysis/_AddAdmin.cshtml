    

<div id="cd-AddAdmin">
    <!-- sign up form -->
    <form class="cd-form" id="cd-form-AddAdmin">
        @Html.AntiForgeryToken()
        <p class="fieldset">
            <label class="image-replace cd-UserName" for="ddUserName">Select User</label>
            @Html.DropDownList("UserListAdd", new SelectList(new List<string>()), "Select User", new { @class = "full-width has-padding has-border UserListAdd", multiple = "multiple" })
        </p>
        <p class="fieldset">
            <input id="btnAddAdmin" class="full-width has-padding" type="submit" value="Add Admin">
            <span id="AddAdminValidate"></span>
        </p>
    </form>
</div>
